def __test_switch() {
    assert_eq(
        switch 1 {
            1 => 'one'
            2 => 'two'
            10 => 'ten'
        },
        'one',
    )
    assert_eq(
        switch 10 {
            1 => 'one'
            2 => 'two'
            10 => 'ten'
        },
        'ten',
    )
    assert_eq(
        switch 5 {
            1 => 'one'
            2 => 'two'
            10 => 'ten'
        },
        nil,
    )
    assert_eq(
        switch 5 {
            1 => 'one'
            2 => 'two'
            10 => 'ten'
            => 'other'
        },
        'other',
    )
}

def __test_switch_str() {
    assert_eq(
        switch 'a' {
            'a' => 1
            'b' => 2
        },
        1,
    )
    assert_eq(
        switch 'abc' {
            'a' => 1
            'b' => 2
        },
        nil,
    )
    assert_eq(
        switch 'b' {
            'a' => 1
            'b' => 2
        },
        2,
    )
    assert_eq(
        switch ('a' + 'b') {
            'ab' => 1
            'b' => 2
        },
        1,
    )
}
